      "accessKey" : {
        "in" : "query",
        "name" : "accessKey",
        "required" : true,
        "description" : "The identifier of the workload making the request",
        "schema" : {
          "type" : "string"
        }
      },
      "signature" : {
        "in" : "query",
        "name" : "signature",
        "required" : true,
        "description" : "The hash of the arguments excluding the signature keyed with the private key associated with the access key.",
